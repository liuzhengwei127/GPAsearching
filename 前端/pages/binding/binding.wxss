/* pages/binding/binding.wxss */

.weui-cells_after-title{margin-top:0}
.weui-cells{position:relative;margin-top:1.17647059em;background-color:#fff;line-height:1.41176471;font-size:17px}
.weui-cell{padding:10px 15px;position:relative;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}
.weui-cell_input{padding-top:0;padding-bottom:0}
.weui-label{width:105px;word-wrap:break-word;word-break:break-all}
.weui-input{height:2.58823529em;min-height:2.58823529em;line-height:2.58823529em}
.weui-cells2{position:relative;margin-top:0.8em;background-color:#fff;line-height:1.41176471;font-size:17px}
.button-sp-area{
    margin: 0 auto;
    padding-top: 10px;
    width: 50%;
}
.weui-btn{margin-top:15px}.weui-btn:first-child{margin-top:5px}.weui-btn-area{margin:1.17647059em 10px .3em}
.weui-footer{color:#999;font-size:10px;text-align:center}
.weui-footer_fixed-bottom{position:fixed;bottom:.52em;left:0;right:0}
.weui-footer__links{font-size:0}
.weui-footer__link{display:inline-block;vertical-align:top;margin:0 .62em;position:relative;font-size:14px;color:#586c94}.weui-footer__link:before{content:" ";position:absolute;left:0;top:0;width:1px;bottom:0;border-left:1rpx solid #c7c7c7;color:#c7c7c7;left:-.65em;bottom:0}
.weui-footer__link:first-child:before{display:none}
.weui-footer__text{padding:0 .34em;font-size:5px}