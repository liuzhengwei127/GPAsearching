<!--pages/score/score.wxml-->

 <view class="page__hd">
        <view class="page__title">我的成绩</view>
        <view class="page__desc">最后更新时间 :{{updateTime}}</view>
    </view>
<view class="page">
     <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">姓名</view>
                <view class="weui-cell__ft">{{Name}}</view>
            </view>
      </view>
      <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">学号</view>
                <view class="weui-cell__ft">{{ID}}</view>
            </view>
      </view>
      <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">学积分</view>
                <view class="weui-cell__ft">{{score}}</view>
            </view>
      </view>
      <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">学积分排名</view>
                <view class="weui-cell__ft">{{scoreRank}}</view>
            </view>
      </view>
      <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell">
                <view class="weui-cell__bd">核心课程总学分</view>
                <view class="weui-cell__ft">{{credit}}</view>
            </view>
      </view>
</view>
<view class="weui-footer weui-footer_fixed-bottom">
            <view class="weui-footer__links">
                <text bindtap="_about" class="weui-footer__link">关于</text>
                <text bindtap="_help" class="weui-footer__link">帮助</text>
                <text bindtap="_privacy" class="weui-footer__link">隐私说明</text>
            </view>
</view>